{
  "gtie": {
    "type": "doc",
    "folder": "gtie"
  },
  "rjs": {
    "type": "doc",
    "folder": "rjs",
    "postinstalls": ["quarto add quarto-ext/attribution --quiet --no-prompt"]
  },
  "main": {
    "type": "doc",
    "folder": "main"
  },
  "springernature": {
    "type": "doc",
    "folder": "journals/springernature"
  },
  "elsevier": {
    "type": "doc",
    "folder": "journals/elsevier"
  },
  "3dpam": {
    "type": "doc",
    "folder": "journals/3dpam"
  },
  "jmd": {
    "type": "doc",
    "folder": "journals/jmd"
  },
  "idetc": {
    "type": "doc",
    "folder": "journals/idetc"
  },
  "psu": {
    "type": "doc",
    "folder": "journals/psu"
  },
  "resume": {
    "type": "doc",
    "folder": "portfolio/resume"
  },
  "vitae": {
    "type": "doc",
    "folder": "portfolio/vitae"
  },
  "letter": {
    "type": "doc",
    "folder": "portfolio/letter"
  },
  "pyr": {
    "type": "doc",
    "folder": "pyr",
    "destination": "./"
  },
  "shadcn": {
    "type": "app",
    "folder": "shadcn",
    "scripts": {
      "shadd": "bunx --bun shadcn@latest add --overwrite --yes --silent"
    }
  },
  "auth": {
    "type": "app",
    "folder": "auth",
    "dependencies": [
      "better-auth",
      "@daveyplate/better-auth-ui",
      "drizzle-orm",
      "@neondatabase/serverless",
      "dotenv"
    ],
    "devDependencies": ["drizzle-kit", "tsx"],
    "scripts": {
      "auth:db:generate": "bunx @better-auth/cli generate",
      "auth:db:migrate": "bunx @better-auth/cli migrate",
      "db:studio": "bun drizzle-kit studio"
    }
  },
  "db": {
    "type": "app",
    "folder": "db",
    "dependencies": [
      "@tanstack/react-query",
      "drizzle-orm",
      "@neondatabase/serverless",
      "dotenv"
    ],
    "devDependencies": ["drizzle-kit", "tsx"],
    "scripts": {
      "db:generate": "bun drizzle-kit generate",
      "db:migrate": "bun drizzle-kit migrate",
      "db:studio": "bun drizzle-kit studio"
    },
    "extras": {
      "dependencies": [
        "@tanstack/react-db",
        "@tanstack/query-db-collection",
        "@tanstack/electric-db-collection"
      ]
    }
  },
  "form": {
    "type": "app",
    "folder": "form",
    "dependencies": ["@tanstack/react-form", "sonner", "zod"]
  },
  "tauri": {
    "type": "app",
    "devDependencies": ["@tauri-apps/cli"],
    "postinstalls": [
      "bunx --bun tauri init && bunx --bun tauri icon .\\public\\icons\\icon.ico"
    ],
    "scripts": {
      "taurize": "bun exec tauri build"
    }
  },
  "r3f": {
    "type": "app",
    "folder": "r3f",
    "dependencies": [
      "@react-three/xr@latest",
      "@react-three/drei",
      "@react-three/fiber",
      "@react-three/uikit",
      "r3f-perf",
      "@types/three",
      "three",
      "three-stdlib"
    ],
    "scripts": {
      "r3add": "bunx --bun uikit component add --overwrite --path components/r3f default"
    },
    "extras": {
      "dependencies": [
        "@react-three/flex",
        "@react-three/rapier",
        "@react-three/offscreen",
        "@react-three/csg",
        "@react-three/postprocessing",
        "three-bvh-csg"
      ]
    }
  },
  "ai": {
    "type": "app",
    "postinstalls": ["bun add @tanstack/ai @tanstack/ai-react"],
    "extras": {
      "postinstalls": ["bun add @tanstack/ai-ollama @tanstack/ai-gemini"]
    }
  }
}
